{
    "name": "racket",
    "displayName": "Racket",
    "description": "Rich support for Racket, inlcuding syntax highlighting and basic code completion",
    "version": "1.0.0",
    "preview": false,
    "publisher": "karyfoundation",
    "licence": "SEE LICENSE IN LICENSE",
    "icon": "icon.png",
    "galleryBanner": {
        "color": "#3e61a3",
        "theme": "light"
    },
    "keywords": [
        "racket",
        "lisp",
        "drracket",
        "scheme"
    ],
    "engines": {
        "vscode": "^1.22.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pmkary/vscode-racket"
    },
    "homepage": "https://github.com/pmkary/vscode-racket/blob/master/README.md",
    "bugs": {
        "url": "https://github.com/pmkary/vscode-racket/issues",
        "email": "pouya@kary.us"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:racket",
        "onCommand:extension.sayHello"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "configurationDefaults": {
            "[racket]": {
                "editor.rulers": [
                    106
                ]
            }
        },
        "languages": [
            {
                "id": "racket",
                "extensions": [
                    ".rkt"
                ],
                "aliases": [
                    "Racket",
                    "racket"
                ],
                "filenames": [
                    ".rkt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "racket",
                "scopeName": "source.racket",
                "path": "./syntaxes/racket.tmLanguage.json"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "node ./node_modules/vscode/bin/compile",
        "compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
    },
    "devDependencies": {
        "typescript": "^1.6.2",
        "vscode": "0.10.x"
    }
}